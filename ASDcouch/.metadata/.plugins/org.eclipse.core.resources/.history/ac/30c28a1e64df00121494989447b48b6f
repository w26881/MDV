function(doc) {
  if (doc.status !== "") {
    emit([doc._id, doc._rev], {
    	"status": doc.status,
    	"name": doc.name,
    	"tel": doc.tel,
    	"notes": doc.notes
    });
  } else
  	if (doc.status === "Current") {
    emit([doc._id, doc._rev], {
    	"status": doc.status,
    	"name": doc.name,
    	"tel": doc.tel,
    	"notes": doc.notes
    });
  } else
  	if (doc.status === "Prospect") {
    emit([doc._id, doc._rev], {
    	"status": doc.status,
    	"name": doc.name,
    	"tel": doc.tel,
    	"notes": doc.notes
    });
  } 
};